{
    "categories": [
      {
        "title": "## 🚀 Features",
        "labels": ["enhancement"]
      },
      {
        "title": "## 🐛 Fixes",
        "labels": ["bug"]
      }
    ],
    "ignore_labels": [
      "ignore"
    ],
    "sort": "ASC",
    "template": "${{CHANGELOG}}\n\n## 👻 Other\n\n${{UNCATEGORIZED}}\n",
    "pr_template": "- ${{TITLE}}: #${{NUMBER}} by ${{AUTHOR}}",
    "empty_template": "No changes",
    "tag_resolver": {
      "method": "semver"
    },
    "base_branches": [
      "master"
    ]
}
